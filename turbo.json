{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [
        "**/.env.*local"
    ],
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
            ]
        },
        "web#build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "api/index.js",
                "api/build/**",
                "public/build/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "start": {
            "outputs": []
        }
    }
}
